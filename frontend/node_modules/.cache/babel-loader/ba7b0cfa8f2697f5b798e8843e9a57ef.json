his.nextPage=res.data.nextPage;\n              this.navigateLastPage=res.data.pages;\n              this.pageNum=res.data.pageNum;\n              console.log(this.boardList);\n              console.log(typeof(res.data.list));\n            })\n            .catch((e)=>console.log(e))\n      }else{\n      this.axios.get(\"/api/board/page\",{params})\n          .then((res)=>{\n            this.boardList=res.data.list;\n            this.prePage=res.data.prePage;\n            this.nextPage=res.data.nextPage;\n            this.pageNum=res.data.pageNum;\n            console.log(res.data);\n            /*console.log(res.data.list)*/})\n          .catch((e)=>console.log(e))\n      }\n    }\n  }\n  // pageLink(){\n  //     this.$router.push({path:'noticeWrite',query:{seq:0}})\n  // }\n}\n</script>\n<style lang=\"scss\" scoped>\n.btn-write {\n    position: absolute;\n    top: 25px;\n    right: 40px;\n    box-sizing: border-box;\n    appearance: none;\n    background-color: transparent;\n    border: 2px solid rgb(186,191,206);\n    border-radius: 0.6em;\n    color: rgb(186,191,206);\n    cursor: pointer;\n    display: flex;\n    align-self: center;\n    font-size: 0.875rem;\n    font-weight: 600;\n    line-height: 1;\n    padding: .7em 1.5em;\n    text-decoration: none;\n    text-align: center;\n    text-transform: uppercase;\n    font-family: 'Montserrat', sans-serif;\n    transition: box-shadow 300ms ease-in-out, color 300ms ease-in-out;\n    &:hover,\n    &:focus {\n        box-shadow: 0 0 40px 40px rgb(186,191,206) inset;\n        outline: 0;\n        color: rgba(255, 255, 255, 1);\n    }\n}\n.board_tbl {\n    color: #000;\n    width: 100%;\n    font-size: 14px;\n    color: #27282d;\n    font-weight: 400;\n    text-align: center;\n    letter-spacing: -.25px;\n    border-collapse: collapse;\n    caption, legend {\n        visibility: hidden;\n        width: 0;\n        height: 0;\n        font-size: 0;\n        line-height: 0;\n    }\n    colgroup col {\n        width: auto!i